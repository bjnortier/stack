{
  "name": "stack",
  "version": "0.1.0",
  "description": "",
  "engines": {
    "node": ">=8.11.3",
    "npm": ">=5.6.0"
  },
  "main": "index.js",
  "scripts": {
    "test:unit": "mocha --require @babel/register --recursive test/unit/**",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:api": "source .func-env && mocha --exit --require @babel/register --require @babel/polyfill --recursive test/api/*.test.js",
    "test:api:watch": "npm run test:api -- --watch",
    "test": "npm run test:unit && npm run test:api",
    "webpack": "webpack",
    "webpack:watch": "webpack -d --watch",
    "start-dev": "source .dev-env && nodemon -w src/server --exec babel-node -- ./src/server/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bjnortier/stack.git"
  },
  "author": "Ben Nortier <ben@bjnortier.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bjnortier/stack/issues"
  },
  "homepage": "https://github.com/bjnortier/stack#readme",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "connect-redis": "^3.3.3",
    "express": "^4.16.3",
    "express-async-handler": "^1.1.4",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "express-winston": "^3.0.0",
    "haikunator": "^2.0.0",
    "helmet": "^3.13.0",
    "massive": "^5.4.0",
    "minimui": "^0.3.1",
    "redux": "^4.0.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-regular-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "babel-loader": "^8.0.2",
    "expect": "^23.6.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "standard": "^12.0.1",
    "styled-components": "^3.4.9",
    "styled-normalize": "^8.0.1",
    "supertest": "^3.3.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "before",
      "beforeEach",
      "after",
      "afterEach"
    ]
  }
}
